#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2009.vpi";
S_0x102d00a60 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x102d035a0 .scope module, "counter_tb" "counter_tb" 3 1;
 .timescale 0 0;
v0x102d08020_0 .var "clk", 0 0;
v0x102d080c0_0 .net "cnt", 6 0, v0x102d07170_0;  1 drivers
v0x102d08160_0 .var "rst", 0 0;
S_0x102d03720 .scope module, "dut" "counter" 3 5, 4 1 0, S_0x102d035a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 7 "cnt";
v0x102d00c30_0 .net "clk", 0 0, v0x102d08020_0;  1 drivers
v0x102d07170_0 .var "cnt", 6 0;
v0x102d07210_0 .net "rst", 0 0, v0x102d08160_0;  1 drivers
E_0x102d01310 .event posedge, v0x102d07210_0, v0x102d00c30_0;
    .scope S_0x102d03720;
T_0 ;
    %wait E_0x102d01310;
    %load/vec4 v0x102d07210_0;
    %flag_set/vec4 8;
    %jmp/1 T_0.2, 8;
    %load/vec4 v0x102d07170_0;
    %cmpi/e 99, 0, 7;
    %flag_or 8, 4;
T_0.2;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 7;
    %assign/vec4 v0x102d07170_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x102d07170_0;
    %addi 1, 0, 7;
    %store/vec4 v0x102d07170_0, 0, 7;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x102d035a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x102d08020_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x102d035a0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x102d08020_0;
    %inv;
    %store/vec4 v0x102d08020_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x102d035a0;
T_3 ;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x102d08160_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x102d08160_0, 0, 1;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x102d08160_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x102d08160_0, 0, 1;
    %delay 200, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x102d08160_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x102d08160_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x102d035a0;
T_4 ;
    %vpi_call/w 3 23 "$dumpfile", "d_ff_dump.vcd" {0 0 0};
    %vpi_call/w 3 24 "$dumpvars" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x102d035a0;
T_5 ;
    %delay 1, 0;
    %vpi_call/w 3 29 "$display", " time | rst | cnt" {0 0 0};
    %vpi_call/w 3 30 "$display", "-------------------" {0 0 0};
    %vpi_call/w 3 31 "$monitor", "%4t   | %b  | %d ", $time, v0x102d08160_0, v0x102d080c0_0 {0 0 0};
    %delay 1500, 0;
    %vpi_call/w 3 32 "$finish" {0 0 0};
    %end;
    .thread T_5;
